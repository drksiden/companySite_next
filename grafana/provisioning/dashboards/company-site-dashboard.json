{
  "dashboard": {
    "title": "Company Site - Application Logs",
    "tags": ["company-site", "logs"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "Error Logs",
        "type": "logs",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "{app=\"company-site-next\", level=\"error\"}",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "loki"
            }
          }
        ],
        "options": {
          "dedupStrategy": "none",
          "enableLogDetails": true,
          "prettifyLogMessage": false,
          "showCommonLabels": false,
          "showLabels": false,
          "showTime": true,
          "sortOrder": "Descending",
          "wrapLogMessage": false
        }
      },
      {
        "id": 2,
        "title": "All Logs",
        "type": "logs",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "{app=\"company-site-next\"}",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "loki"
            }
          }
        ],
        "options": {
          "dedupStrategy": "none",
          "enableLogDetails": true,
          "prettifyLogMessage": false,
          "showCommonLabels": false,
          "showLabels": false,
          "showTime": true,
          "sortOrder": "Descending",
          "wrapLogMessage": false
        }
      },
      {
        "id": 3,
        "title": "Logs by Level",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "sum(count_over_time({app=\"company-site-next\", level=\"error\"}[5m]))",
            "refId": "A",
            "legendFormat": "Errors",
            "datasource": {
              "type": "loki",
              "uid": "loki"
            }
          },
          {
            "expr": "sum(count_over_time({app=\"company-site-next\", level=\"warn\"}[5m]))",
            "refId": "B",
            "legendFormat": "Warnings",
            "datasource": {
              "type": "loki",
              "uid": "loki"
            }
          },
          {
            "expr": "sum(count_over_time({app=\"company-site-next\", level=\"info\"}[5m]))",
            "refId": "C",
            "legendFormat": "Info",
            "datasource": {
              "type": "loki",
              "uid": "loki"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "list",
              "placement": "bottom"
            }
          }
        }
      },
      {
        "id": 4,
        "title": "HTTP Requests",
        "type": "logs",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "{app=\"company-site-next\", level=\"http\"}",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "loki"
            }
          }
        ],
        "options": {
          "dedupStrategy": "none",
          "enableLogDetails": true,
          "prettifyLogMessage": false,
          "showCommonLabels": false,
          "showLabels": false,
          "showTime": true,
          "sortOrder": "Descending",
          "wrapLogMessage": false
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "editable": true,
    "fiscalYearStartMonth": 0,
    "graphTooltip": 0,
    "links": [],
    "liveNow": false,
    "weekStart": ""
  }
}
